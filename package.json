{
	"name": "eliza-health-tracker",
	"private": true,
	"version": "1.0.0",
	"description": "A privacy-first health tracking app for monitoring ailments and triggers. 健康追蹤應用程式。",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"cap:init": "cap init 'Health Tracker' com.healthtracker.app --web-dir build",
		"cap:android": "cap add android",
		"cap:sync": "cap sync",
		"cap:open": "cap open android",
		"mobile:build": "bun run build && cap sync"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@iconify/svelte": "^5.1.0",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"dexie": "^4.2.1",
		"lucide-svelte": "^0.555.0",
		"mode-watcher": "^1.1.0",
		"svelte-sonner": "^1.0.6",
		"tailwind-merge": "^3.4.0"
	}
}
